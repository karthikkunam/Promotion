{
  "name": "7boss-promo",
  "version": "1.0.0",
  "description": "7BOSS Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/server.js",
  "scripts": {
    "clean:libs": "rm -rf node_modules && rm -rf yarn.lock && rm -rf package-lock.json && rm -rf dist",
    "preinstall": "git config --global url.https://github.com/.insteadOf git://github.com/",
    "build": "webpack --mode production && npm run zip",
    "build:prodlocal": "webpack --mode production",
    "build:docker:local": "docker build --rm -t local-promo .",
    "start:docker:local": "docker run -p 5000:5000 -t local-promo",
    "build:dev": "webpack --mode development --config webpack.config.dev.js",
    "start": "npm run build:dev && node -r esm src/server/server.js",
    "client": "webpack-dev-server --mode development --config webpack.config.dev.js --watch --devtool inline-source-map --hot",
    "server": "nodemon -r esm src/server/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "e2e": "node_modules\\.bin\\cucumber-js  src/client/tests/uitests/features/features/*.feature -f json:src/client/tests/uitests/reporter/cucumber_reporter.json && node src/client/tests/uitests/reporter.js",
    "zip": "find . -print0 | xargs -0 touch -m -t 198001010000.00 && zip -X -rq archive.zip ./dist/ -x '/test/*'"
  },
  "proxy": "http://localhost:5000",
  "author": "7Bosspromo",
  "license": "ISC",
  "dependencies": {
    "@7eleven/7boss-components": "^1.0.33",
    "@7eleven/7boss-logger": "^1.1.6",
    "@7eleven/7boss-secrets-manager": "^1.0.4",
    "@7eleven/7boss-style": "^1.1.14",
    "@babel/polyfill": "^7.8.7",
    "@babel/register": "^7.9.0",
    "ag-grid-community": "^23.0.2",
    "ag-grid-enterprise": "^23.0.2",
    "ag-grid-react": "^23.0.2",
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "chalk": "^3.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "helmet": "^3.22.0",
    "json-server": "^0.16.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-vibrant": "^3.1.5",
    "path": "^0.12.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dom-factories": "^1.0.2",
    "react-hot-loader": "^4.12.20",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "reactstrap": "^8.4.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.2",
    "resolve-url-loader": "^3.1.1",
    "watch": "^1.0.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.3",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^2.0.2",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-html-reporter": "^0.7.4",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.0.3",
    "lowdb": "^1.0.0",
    "moment-timezone": "^0.5.28",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "url-loader": "^4.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
